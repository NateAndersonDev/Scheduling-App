<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="MainScenePane" prefHeight="400.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <AnchorPane layoutX="22.0" prefHeight="400.0" prefWidth="333.0">
         <children>
            <Text layoutX="93.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
               <font>
                  <Font name="Courier New" size="20.0" />
               </font>
            </Text>
            <Text layoutX="25.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text layoutX="28.0" layoutY="212.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text layoutX="37.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current Location">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="LoginUserNameField" layoutX="99.0" layoutY="144.0" promptText="Case-Sensitive!" />
            <TextField fx:id="LoginPasswordField" layoutX="99.0" layoutY="194.0" promptText="Case-Sensitive!" />
            <TextField fx:id="CurrentLocationTextField" editable="false" layoutX="140.0" layoutY="307.0" prefHeight="25.0" prefWidth="67.0" />
            <Button fx:id="LoginFormLoginBTN" layoutX="47.0" layoutY="242.0" mnemonicParsing="false" text="Login" />
            <Button fx:id="LoginFormCancelBTN" layoutX="159.0" layoutY="242.0" mnemonicParsing="false" onAction="#exitApplication" text="Cancel" />
            <Label layoutX="-17.0" layoutY="119.0" text="Login invalid, please check username and/or password for errors" textFill="#d31414" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
