<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="MainScenePane" prefHeight="400.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="295.0">
         <children>
            <Text layoutX="93.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%Login">
               <font>
                  <Font name="Courier New" size="20.0" />
               </font>
            </Text>
            <Text layoutX="8.0" layoutY="161.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%Username">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text layoutX="10.0" layoutY="211.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text layoutX="37.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%Location">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="LoginUserNameField" layoutX="141.0" layoutY="142.0" promptText="Case-Sensitive!" />
            <TextField fx:id="LoginPasswordField" layoutX="141.0" layoutY="192.0" promptText="Case-Sensitive!" />
            <Button fx:id="LoginFormLoginBTN" layoutX="63.0" layoutY="242.0" mnemonicParsing="false" onAction="#LoginBtnPress" text="%Login" />
            <Button fx:id="LoginFormCancelBTN" layoutX="177.0" layoutY="242.0" mnemonicParsing="false" onAction="#exitApplication" text="%Cancel" />
            <Label fx:id="FxError" layoutX="37.0" layoutY="116.0" text="%Error" textAlignment="CENTER" textFill="#313335" />
            <Label fx:id="ZoneIDField" layoutX="105.0" layoutY="311.0" text="Label" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
